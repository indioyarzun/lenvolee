    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.10.0",
    "next": "15.1.4",
    "payload": "3.15.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.5.5"
