    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8",
    "eslint-config-next": "15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^25.0.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
